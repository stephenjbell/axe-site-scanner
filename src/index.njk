---
title: Scanned Pages
---

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>{{title}} | {{site.name}}</title>
    <link rel="stylesheet" href="/css/site.css">
</head>
<body>
    <h1>{{site.name}}</h1>

    <details>
        <summary>Crawled pages</summary>
        <ul>
            {% for page in scanner.crawledPages %}
            <li>
                <a href="{{page.url}}">
                    {{page.url}}
                    {%- if page.innav -%}
                        *
                    {%- endif -%}
                </a>
            </li>
            {% endfor %}
        </ul>
    </details>

    <ul>
        {% for page in scanner.resultPages %}
            <li>{{page.results.url}}<br>

                {% set resultTypes = ["violations","incomplete"] %}

                {% for resultType in resultTypes %}
                    <p>
                        {{resultType}}: {{page.results[resultType]|length}}
                        <ul>
                            {% for item in page.results[resultType] %}
                                <li>{{item.id}} - {{item.nodes|length}}</li>

                                {% for node in item.nodes %}
                                    - {{ node.target[0] }} <br>
                                {% endfor %}
                            {% endfor %}
                        </ul>
                    </p>
                {% endfor %}

                
            </li>
        {% endfor %}
    </ul>

    <p>
        Scanned with {{scanner.resultPages[0].results.testEngine.name}} {{scanner.resultPages[0].results.testEngine.version}}
        on {{ helpers.formatDate( scanner.resultPages[0].results.timestamp, "ff" ) }}
    </p>

    <pre>
    {{scanner.resultPages|dump(4)}}
    </pre>

</body>
</html>